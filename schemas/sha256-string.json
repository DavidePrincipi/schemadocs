{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "SHA256 string format",
    "description": "A SHA256 string is 64 hexadecimal numbers long",
    "examples": [
        "73cb3858a687a8494ca3323053016282f3dad39d42cf62ca4e79dda2aac7d9ac"
    ],
    "type": "string",
    "minLength": 64,
    "maxLength": 64,
    "pattern": "^[a-f0-9]{64}$"
}
